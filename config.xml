<?xml version='1.0' encoding='utf-8'?>
<widget id="com.rma.googlemapstest" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>GoogleMaps Test</name>
    <description>
        Aplicaci√≥n de ejemplo para probar el plugin "cordova-plugin-googlemaps"
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="AndroidXEnabled" value="true" />
        <preference name="android-minSdkVersion" value="22" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <!--Google Maps API KEY -->
    <preference name="GOOGLE_MAPS_ANDROID_API_KEY" value="" />
    <preference name="GOOGLE_MAPS_IOS_API_KEY" value="" />
</widget>
